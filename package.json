{
	"name": "@tammergard/eslint-config-base",
	"version": "0.0.0-development",
	"description": "A sharable ESLint config for JavaScript with personal preferences",
	"scripts": {
		"format": "prettier --write .",
		"format-check": "prettier --check .",
		"lint": "eslint .",
		"review": "yarn install --frozen-lockfile && yarn format-check && yarn lint"
	},
	"dependencies": {
		"eslint-config-airbnb-base": "15.0.0",
		"eslint-config-prettier": "8.6.0",
		"eslint-plugin-import": "2.27.5",
		"eslint-plugin-prettier": "4.2.1"
	},
	"devDependencies": {
		"@tammergard/prettier-config": "2.0.0",
		"@tammergard/semantic-release-config-npm": "1.1.0",
		"eslint": "8.32.0",
		"prettier": "2.8.3",
		"semantic-release": "20.0.2"
	},
	"peerDependencies": {
		"eslint": ">=7.32.0",
		"prettier": ">=2.4.1"
	},
	"main": "index.js",
	"files": [
		"index.js"
	],
	"publishConfig": {
		"access": "public"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/filiptammergard/eslint-config-base.git"
	},
	"keywords": [
		"eslint",
		"eslint-config",
		"config",
		"style-guide",
		"javascript"
	],
	"author": "Filip Tammerg√•rd <filip+npm@tammergard.se> (https://tammergard.se/)",
